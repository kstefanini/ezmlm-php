{
	"ezmlm": {
		"domainsPath": "/home/vpopmail/domains",
		"domain": "localhost"
	},
	"ezmlm-idx": {
		"binariesPath": "/usr/local/bin/ezmlm"
	},
	"system": {
		"grepBinary": "/bin/grep",
		"findBinary": "/usr/bin/find"
	},
	"authAdapter": "",
	"adapters": {
		"AuthProxyTB": {
			"annuaireURL": "https://localhost/service:annuaire:auth",
			"admins": [
				"john@mywebsite.org"
			],
			"adminRoles": [
				"tb_ezmlm-php_admin"
			],
			"ignoreSSLIssues": false,
			"headerName": "Authorization"
		}
	},
	"settings": {
		"maxMessagesWithContentsReadableAtOnce": 100,
		"messageAbstractSize": 64
	},
	"sendMode": "mail",
	"smtp": {
		"host": "localhost",
		"port": 25,
		"auth": false,
		"user": "",
		"password": "",
		"secure": "tls",
		"debug": 3
	},
	"cache": {
		"path": "/tmp/ezmlm-php"
	}
}
